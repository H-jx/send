{
  "name": "ff-send",
  "version": "1.0.0",
  "author": "flynn",
  "license": "MIT",
  "browserslist": "> 0.5%, last 10 versions, not dead",
  "scripts": {
    "client:dev": "rimraf dist/client && parcel src/client/index.html --dist-dir ./dist/client --host 0.0.0.0",
    "client:build": "parcel build src/client/index.html --dist-dir dist/client",
    "rpc:server": "yarn ts-brpc server -c ./src/ts-brpc.json",
    "rpc:client": "yarn ts-brpc client -c ./src/ts-brpc.json",
    "server:dev": "yarn rpc:server && cross-env NODE_ENV=development nodemon --config nodemon.json",
    "server:build": "yarn rpc:server && yarn gulp",
    "start": "node dist/server/app.js",
    "build": "yarn client:build && yarn server:build",
    "gulp": "gulp build",
    "build:c": "tsc",
    "pm2:prod": "pm2 start ecosystem.config.js --env production",
    "docker-compose:dev": "docker-compose -f docker-compose.dev.yml up  -d --force-recreate",
    "rm-modules": "rimraf node_modules",
    "lint": "eslint ./src --ext .ts,.tsx --quiet --fix",
    "tslint": "tsc --project ./tsconfig.server.json --noEmit",
    "test": "jest --colors"
  },
  "dependencies": {
    "@koa/cors": "^3.1.0",
    "antd-mobile": "^5.0.0",
    "antd-mobile-icons": "^0.2.2",
    "classnames": "^2.3.1",
    "clipboard": "^2.0.11",
    "core-js": "3",
    "dayjs": "^1.10.7",
    "ioredis": "^4.28.5",
    "koa": "^2.13.4",
    "koa-body": "^4.2.0",
    "koa-router": "^10.1.1",
    "koa-static": "^5.0.0",
    "koa2-connect-history-api-fallback": "^0.1.3",
    "lodash": "^4.17.21",
    "log4js": "^6.9.1",
    "minio": "^7.0.28",
    "module-alias": "^2.2.2",
    "qrcode": "^1.5.0",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-file-icon": "^1.1.0",
    "react-router-dom": "^6.2.1",
    "redis": "^4.0.4",
    "socket.io": "^4.4.1",
    "socket.io-client": "^4.4.1",
    "spark-md5": "^3.0.2",
    "ts-brpc": "^1.1.0",
    "use-easy-model": "^1.0.5",
    "uuid": "^8.3.2"
